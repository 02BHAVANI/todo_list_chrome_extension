{
  "manifest_version": 3,
  "name": "Todo List Extension",
  "description": "A simple to-do list Chrome extension built with React",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
